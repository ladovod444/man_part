<style>
    @media print {
        body {
            visibility: hidden;
        }

        .main {
            display: none;
        }

        .printer {
            position: absolute;
            visibility: visible !important;
        }

        .modal-body {
            position: fixed !important;
            top: 0;
            left: 0;
        }

        .page-break {
            position: unset !important;
        }
    }
</style>

